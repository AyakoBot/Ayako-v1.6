{
  "name": "@ayako/bot",
  "version": "2.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "swc src -d dist --strip-leading-paths --copy-files",
    "dev": "node --enable-source-maps --experimental-wasm-modules --experimental-json-modules ./dist/index.js --debug --warn --dev",
    "postinstall": "mkdir -p ./logs",
    "lint": "npx eslint 'src/**/*.ts' --fix",
    "register": "node ./dist/register-commands.js",
    "run": "clinic heapprofiler --collect-only --open=false -- node --max-old-space-size=10240 --no-deprecation --no-warnings --experimental-json-modules ./dist/index.js",
    "prestart": "rm -rf dist && pnpm build",
    "start": "pnpm run run",
    "watch": "swc src -d dist --strip-leading-paths --copy-files --watch"
  },
  "dependencies": {
    "@discordjs/builders": "1.9.0-dev.1720743039-be04acd53",
    "@discordjs/core": "^1.2.0",
    "@discordjs/rest": "^2.3.0",
    "@prisma/client": "^5.16.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/lodash": "^4.17.6",
    "@types/lodash.merge": "^4.6.9",
    "captcha-canvas": "^3.2.2",
    "client@latest": "link:prisma/client@latest",
    "discord-api-types": "^0.37.92",
    "discord-hybrid-sharding": "^2.2.0",
    "discord.js": "14.16.0-dev.1720915912-efa16a609",
    "dotenv": "^16.4.5",
    "glob": "^10.4.5",
    "ioredis": "^5.4.1",
    "lodash.merge": "^4.6.2",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "ms": "^2.1.3",
    "nekos-best.js": "^6.3.0",
    "node-schedule": "^2.1.1",
    "os": "^0.1.2",
    "purrbot-api": "^1.3.1",
    "string-similarity": "^4.0.4",
    "waifu-pics-api": "^1.0.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.8",
    "@babel/eslint-plugin": "^7.24.7",
    "@swc/cli": "^0.4.0",
    "@swc/core": "1.6.12-nightly-20240706.1",
    "@types/body-parser": "^1.19.5",
    "@types/clone": "^2.1.4",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/moment-duration-format": "^2.2.6",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.14.10",
    "@types/node-schedule": "^2.1.7",
    "@types/request": "^2.48.12",
    "@types/source-map-support": "^0.5.10",
    "@types/string-similarity": "^4.0.2",
    "@types/valid-url": "^1.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^9.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "prisma": "^5.16.2",
    "source-map-support": "^0.5.21",
    "typescript": "^5.5.3"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": "^22.3.0",
    "pnpm": "^9.5.0"
  },
  "overrides": {
    "@discordjs/builders": "dev",
    "discord-api-types": "next"
  },
  "prisma": {
    "schema": "../../prisma/schema.prisma"
  }
}
