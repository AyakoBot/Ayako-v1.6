  lvlupdeltimeout Decimal     @default(10) @db.Decimal
  lvlupchannels   String[]    @db.VarChar
  lvlupemotes     String[]    @default(dbgenerated("'{🆙,AMayakopeek:924071140257841162}'::character varying[]")) @db.VarChar
