{
 "name": "@ayako/bot",
 "version": "2.4.6",
 "private": true,
 "type": "module",
 "scripts": {
  "build": "swc src -d dist --strip-leading-paths --copy-files",
  "dev": "pnpx prisma db push && node --enable-source-maps --experimental-wasm-modules --experimental-json-modules ./dist/index.js --debug --warn --dev",
  "postinstall": "mkdir -p ./logs",
  "lint": "npx eslint 'src/**/*.ts' --fix",
  "register": "node ./dist/register-commands.js",
  "run": "node --max-old-space-size=10240 --no-deprecation --no-warnings --experimental-json-modules ./dist/index.js",
  "start": "pnpm run register && pnpm run run",
  "watch": "swc src -d dist --strip-leading-paths --copy-files --watch"
 },
 "dependencies": {
  "@discordjs/core": "^2.0.0",
  "@discordjs/rest": "^2.4.0",
  "@prisma/client": "^6.1.0",
  "@total-typescript/ts-reset": "^0.6.1",
  "@types/lodash": "^4.17.13",
  "@types/lodash.merge": "^4.6.9",
  "captcha-canvas": "^2.3.1",
  "client@latest": "link:prisma/client@latest",
  "discord-api-types": "0.37.111",
  "discord-hybrid-sharding": "^2.2.3",
  "discord.js": "14.16.1",
  "dotenv": "^16.4.7",
  "glob": "^11.0.0",
  "ioredis": "^5.4.1",
  "lodash.merge": "^4.6.2",
  "moment": "^2.30.1",
  "moment-duration-format": "^2.3.2",
  "ms": "^2.1.3",
  "nekos-best.js": "^6.3.0",
  "node-schedule": "^2.1.1",
  "os": "^0.1.2",
  "prom-client": "^15.1.3",
  "purrbot-api": "^1.3.1",
  "string-similarity": "^4.0.4",
  "waifu-pics-api": "^1.0.5"
 },
 "devDependencies": {
  "@babel/eslint-parser": "^7.25.9",
  "@babel/eslint-plugin": "^7.25.9",
  "@swc/cli": "^0.5.2",
  "@swc/core": "1.10.1",
  "@types/body-parser": "^1.19.5",
  "@types/clone": "^2.1.4",
  "@types/glob": "^8.1.0",
  "@types/lodash.clonedeep": "^4.5.9",
  "@types/moment-duration-format": "^2.2.6",
  "@types/ms": "^0.7.34",
  "@types/node": "^22.10.2",
  "@types/node-schedule": "^2.1.7",
  "@types/request": "^2.48.12",
  "@types/source-map-support": "^0.5.10",
  "@types/string-similarity": "^4.0.2",
  "@types/valid-url": "^1.0.7",
  "@types/ws": "^8.5.13",
  "@typescript-eslint/eslint-plugin": "^8.18.1",
  "@typescript-eslint/parser": "^8.18.1",
  "eslint": "^8.57.1",
  "eslint-config-airbnb": "^19.0.4",
  "eslint-config-prettier": "^9.1.0",
  "eslint-import-resolver-typescript": "^3.7.0",
  "eslint-plugin-import": "^2.31.0",
  "eslint-plugin-json": "^4.0.1",
  "eslint-plugin-jsx-a11y": "^6.10.2",
  "eslint-plugin-prettier": "^5.2.1",
  "prettier": "^3.4.2",
  "prisma": "^6.1.0",
  "source-map-support": "^0.5.21",
  "typescript": "^5.7.2"
 },
 "packageManager": "pnpm@9.9.0",
 "engines": {
  "node": "^22.3.0",
  "pnpm": "^9.5.0"
 },
 "overrides": {
  "discord.js": {
   "dependencies": {
    "discord-api-types": "0.37.111"
   }
  }
 },
 "prisma": {
  "schema": "../../prisma/schema.prisma"
 }
}
